    "import numpy as np\n",
    "import scipy.special\n",
    "import math\n",
    "\n",
    "def Mac_ser(x,n):\n",
    "    \n",
    "    N = np.arange(n)\n",
    "    return np.sum(x**N/scipy.special.factorial(N))\n",
    "    \n",
    "        "
